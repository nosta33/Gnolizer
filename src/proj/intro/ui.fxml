<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="center" prefHeight="420.0" prefWidth="380" spacing="40.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proj.intro.ControllerIntro">
	
	<Text text="GnoLyser">
      <font>
         <Font name="SansSerif Regular" size="40.0" />
      </font>
      <VBox.margin>
         <Insets />
      </VBox.margin></Text>
   <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
      <children>
         <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
            <children>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Projet :" />
               <ComboBox fx:id="projCombo" prefWidth="200.0" promptText="- Choisir -">
                  <HBox.margin>
                     <Insets left="20.0" right="5.0" />
                  </HBox.margin>
               </ComboBox>
            </children>
            <VBox.margin>
               <Insets left="40.0" right="40.0" />
            </VBox.margin>
         </HBox>
         <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
            <children>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Du :" />
               <TextField fx:id="debutField" alignment="CENTER" promptText="jj/mm/aaaa">
                  <HBox.margin>
                     <Insets left="5.0" right="5.0" />
                  </HBox.margin>
               </TextField>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Au :" />
               <TextField fx:id="finField" alignment="CENTER" promptText="jj/mm/aaaa">
                  <HBox.margin>
                     <Insets left="5.0" right="5.0" />
                  </HBox.margin>
               </TextField>
            </children>
            <VBox.margin>
               <Insets left="40.0" right="40.0" />
            </VBox.margin>
         </HBox>
         <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
            <children>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Graphe :" />
               <ComboBox fx:id="comboGraph" prefWidth="200.0" promptText="- Choisir -">
                  <HBox.margin>
                     <Insets left="15.0" />
                  </HBox.margin>
               </ComboBox>
            </children>
            <VBox.margin>
               <Insets left="40.0" right="40.0" />
            </VBox.margin>
         </HBox>
      </children>
   </VBox>
   <Button fx:id="boutonAfficher" alignment="CENTER" onAction="#process" text="Afficher" />
</VBox>
